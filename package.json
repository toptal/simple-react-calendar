{
  "name": "simple-react-calendar",
  "npmName": "simple-react-calendar",
  "description": "A simple calendar component for React based applications.",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toptal/simple-react-calendar.git"
  },
  "version": "2.0.0",
  "scripts": {
    "start": "webpack-dev-server --env.development --config config/webpack.dev.js --hot --progress",
    "build": "cross-env BABEL_ENV='production' rimraf lib && tsc -p ./tsconfig.build.json",
    "build:docs": "cross-env BABEL_ENV='production' rimraf dist && webpack --env.production --config ./config/webpack.dev.js",
    "prepare": "yarn build",
    "clean": "rimraf ./dist && rimraf ./lib",
    "test": "env NODE_ENV=test TZ=UTC jest",
    "test:watch": "env NODE_ENV=test TZ=UTC jest --watch",
    "test:coverage": "env NODE_ENV=test TZ=UTC jest --coverage",
    "test:ci": "env NODE_ENV=test TZ=UTC jest --bail --coverage --maxWorkers=4 --ci",
    "format:js": "prettier \"+(config|src|docs)/**/*.+(js*|tsx)\" --write",
    "format:md": "prettier \"**/*.+(md*)\" --write",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "bugs": {
    "url": "https://github.com/toptal/simple-react-calendar/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/plugin-transform-react-jsx-source": "^7.2.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^23.3.13",
    "@types/jest-plugin-context": "^2.9.0",
    "@types/react": "^16.7.3",
    "autoprefixer": "^9.4.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "23",
    "babel-loader": "^8.0.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "docz": "^0.13.7",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^0.13.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "husky": "^1.1.3",
    "jest": "23",
    "jest-plugin-context": "^2.9.0",
    "jest-plugins": "^2.9.0",
    "lint-staged": "^8.0.4",
    "luxon": "^1.10.0",
    "mockdate": "^2.0.2",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^23.10.5",
    "ts-lint": "^4.5.1",
    "ts-loader": "^5.3.2",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.2.0",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.14"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "luxon": "^1.0.0",
    "react-dom": "^16.0.0",
    "react": "^16.0.0"
  },
  "lint-staged": {
    "*.{tsx, ts, md, css}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "react",
    "reactjs",
    "simple",
    "calendar",
    "react-component",
    "calendar",
    "full calendar",
    "typescript",
    "luxon",
    "timezone",
    "locale"
  ],
  "files": [
    "README.md",
    "lib",
    "dist"
  ]
}
